    "url": "https://your-website.com"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "WebSocket Server Emulator",
    "copyright": "Copyright Â© 2022 Alexander Velikodnyy",
    "win": {
      "target": [
        "nsis",
        "portable",
        "zip"
      ]
    },
    "portable": {
      "artifactName": "HelloWorld.exe"
    },
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "description": "WebSocket Server Emulator",
  "private": true,
  "main": "public/electron.js",
  "productName": "WebSocket Server Emulator",
  "homepage": "https://belython.github.io/WebSocket-Server-Emulator/",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.86",
    "@mui/material": "^5.8.7",
    "@mui/styled-engine-sc": "^5.8.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.38",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "classnames": "^2.3.1",
    "effector-react": "^22.0.6",
    "electron-is-dev": "^2.0.0",
    "file-save": "^0.2.0",
    "file-system": "^2.2.2",
    "is-electron": "^2.2.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-file-reader": "^1.1.4",
    "react-json-pretty": "^2.2.0",
    "react-native-fs": "^2.0.1-rc.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-localstorage-simple": "^2.5.1",
    "redux-thunk": "^2.4.1",
    "save-file": "^2.3.1",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.4",
    "websocket-ts": "^1.1.1"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"SET BROWSER=none&&yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -w",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.16",
    "@rescripts/rescript-env": "^0.0.14",
    "any-text": "^1.2.0",
    "concurrently": "^7.2.2",
    "electron": "^19.0.7",
    "electron-builder": "^23.1.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.5.1",
    "electron-winstaller": "^5.0.0",
    "jest": "^28.1.2",
